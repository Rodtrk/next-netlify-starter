{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["components/*"],
      "@styles/*": ["styles/*"]
    }
  }
}

async Header(){
  return [
    {
      source : "/(.*)",
      headers :
      {
        key : "X-Content-Type-Options",
        value : "nosniff"
      }
  ]
}
